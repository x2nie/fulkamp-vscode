{
  "name": "fulkamp",
  "displayName": "FulkAmp",
  "description": "Syntax highlighting for FulkAmp",
  "version": "0.0.1",
  "publisher": "x2nie",
  "engines": {
    "vscode": "^1.80.0"
  },
  "scripts": {
    "build": "vsce package"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fulkamp",
        "aliases": [
          "FulkAmp",
          "fulkamp"
        ],
        "extensions": [
          ".fa",
          ".fulkamp",
          "fu.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fulkamp",
        "scopeName": "source.fulkamp",
        "path": "./syntaxes/fulkamp.tmLanguage.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:fulkamp"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
},
"devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.9.3",
    "vscode": "^1.1.37"
  }
}
